{
    "hosting": {
        "source": "frontend",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "frameworksBackend": {
            "region": "europe-west1"
        },
        "rewrites": [
            {
                "source": "/api/**",
                "function": "api"
            }
        ],
        "cleanUrls": true,
        "trailingSlash": false
    }
}